{
  "name": "<%= _.slugify(projectname) %>",
  "description": "<%= projectdesc %>",
  "version": "0.1.0",
  "homepage": "https://github.com/<%= username %>/<%= _.slugify(projectname) %>",
  "author": {
    "name": "<%= authorname %>",
    "url": "<%= authorurl %>"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/<%= username %>/<%= _.slugify(projectname) %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= username %>/<%= _.slugify(projectname) %>/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/<%= username %>/<%= _.slugify(projectname) %>/blob/master/LICENSE-MIT"
    }
  ],
  "keywords": [
    "cli",
    "command line"
  ],
  "preferGlobal": true,
  "bin": {
    "<%= cliname %>": "./bin/<%= cliname %>"
  },
  "main": "index.js",
  "engines": {
    "node": ">=0.8"
  },
  "scripts": {
    "test": "mocha -R test"
  },
  "dependencies": {
    "minimist": "~0.0.8",
    "verbalize": "~0.1.1"
  }<% if (verb) { %>,
  "devDependencies": {
    "verb": "~0.2.0"
  }<% } %>
}